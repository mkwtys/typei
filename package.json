{
  "name": "typei",
  "version": "0.7.0",
  "description": "types interactive installer",
  "keywords": [
    "cli",
    "install",
    "interactive",
    "types",
    "typescript",
    "typings",
    "update"
  ],
  "homepage": "https://github.com/mkwtys/typei",
  "bugs": "https://github.com/mkwtys/typei/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkwtys/typei.git"
  },
  "license": "MIT",
  "author": "mkwtys <mkwtys@gmail.com>",
  "main": "./lib/",
  "bin": {
    "typei": "./bin/typei.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "npm run clean:build && npm run build:js && npm run build:types",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --ignore src/**/__tests__/",
    "build:types": "tsc --emitDeclarationOnly",
    "clean:build": "rimraf lib",
    "clean:coverage": "npm run clean:test && rimraf coverage",
    "clean:test": "rimraf src/__tests__/testProject",
    "coverage": "npm run clean:coverage && jest --coverage",
    "test": "npm run clean:test && jest",
    "test:update": "jest -u",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "node": 10
          },
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "execa": "^5.0.0",
    "inquirer": "^8.0.0",
    "ora": "^5.0.0",
    "package-json": "^7.0.0",
    "path-exists": "^4.0.0",
    "preferred-pm": "^3.0.0",
    "read-pkg": "^5.2.0",
    "semver": "^7.1.1",
    "strip-ansi": "^6.0.0",
    "text-table": "^0.2.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@types/inquirer": "7.3.3",
    "@types/jest": "26.0.23",
    "@types/mkdirp": "1.0.1",
    "@types/node": "14.17.4",
    "@types/preferred-pm": "2.0.0",
    "@types/rimraf": "3.0.0",
    "@types/semver": "7.3.7",
    "@types/text-table": "0.2.2",
    "@types/yargs": "16.0.4",
    "jest": "26.6.3",
    "mkdirp": "1.0.4",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  }
}
