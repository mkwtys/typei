{
  "name": "typei",
  "version": "0.7.0",
  "description": "types interactive installer",
  "keywords": [
    "cli",
    "install",
    "interactive",
    "types",
    "typescript",
    "typings",
    "update"
  ],
  "homepage": "https://github.com/mkwtys/typei",
  "bugs": "https://github.com/mkwtys/typei/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkwtys/typei.git"
  },
  "license": "MIT",
  "author": "mkwtys <mkwtys@gmail.com>",
  "type": "module",
  "main": "./lib/",
  "bin": {
    "typei": "./bin/typei.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "clean:coverage": "npm run clean:test && rimraf coverage",
    "clean:test": "rimraf test/testProject",
    "coverage": "npm run clean:coverage && cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test": "npm run clean:test && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:update": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -u"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    }
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "execa": "^6.1.0",
    "inquirer": "^8.0.0",
    "ora": "^6.0.0",
    "package-json": "^7.0.0",
    "path-exists": "^5.0.0",
    "preferred-pm": "^3.0.0",
    "read-pkg": "^7.0.0",
    "semver": "^7.1.1",
    "strip-ansi": "^7.0.0",
    "text-table": "^0.2.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/jest": "27.5.1",
    "@types/mkdirp": "1.0.2",
    "@types/node": "16.11.35",
    "@types/preferred-pm": "2.0.0",
    "@types/rimraf": "3.0.2",
    "@types/semver": "7.3.9",
    "@types/text-table": "0.2.2",
    "@types/yargs": "17.0.10",
    "cross-env": "7.0.3",
    "jest": "28.1.0",
    "mkdirp": "1.0.4",
    "rimraf": "3.0.2"
  }
}
